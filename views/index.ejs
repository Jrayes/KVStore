<% layout('layout') -%>

<h1>Key Value Store</h1>
<p class="lead">A cloud native kv store<br> </p>

<form class="form-inline mt-2 mt-md-0" action="/store" method="post">
    <input class="form-control mr-sm-2" type="text" name="key" placeholder="Key" aria-label="key">
    <input class="form-control mr-sm-2" type="text" name="value" placeholder="Value" aria-label="value">
    <button id="storebutton" class="btn btn-outline-success my-2 my-sm-0" type="submit"> Set </button>
</form>
