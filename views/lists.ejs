<% layout('layout') -%>

<div class="table-responsive">
    <table id="table-cluster" class="table table-striped">
            <thead>
                <th>Key</th>
                <th>Value</th>
            </thead>
            <tbody>
                <% var count=0; %>
            <% for (const [key, value] of val.entries()) { %>
                <% if (count == limit) { %>
                   <% break; %>
                <% } %>
                    <tr><td><%= key %></td> <td><%= value %></td></tr>
                    <% count++;%>
                <% } %>
            </tbody>
    </table>
</div>


